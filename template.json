        {
          "name": "jinja",
          "options": {
            "template": "src/all_questions/default.html.j2",
            "content-type": "text/html",
            "extension": "html"
          }
        },
        {
          "name": "weasyprint",
          "options": {
          }
        }
      ]
    },
    {
      "uuid": "776aeac2-33f6-4a35-afe5-ddc583302ef7",
      "name": "MS Word Document, all questions, no text, no question extras, no tags",
      "icon": "far fa-file-word",
      "steps": [
        {
          "name": "jinja",
          "options": {
            "template": "src/all_questions/for-word.html.j2",
            "content-type": "text/html",
            "extension": "html"
          }
        },
        {
          "name": "pandoc",
          "options": {
            "from": "html",
            "to": "docx",
            "args": "--standalone --filter=pandoc-docx-pagebreakpy --reference-doc=src/reference.docx"
          }
        }
      ]
    },
